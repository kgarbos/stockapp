{
  "extends": "next",
  "rules": {
    "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off",
    "quotes": "off",
    "linebreak-style": "off",
    "max-len": "off",
    "comma-dangle": "off",
    "object-curly-spacing": "off",
    "indent": "off",
    "eol-last": "off",
    "semi": "off",
    "no-trailing-spaces": "off",
    "arrow-parens": "off",
    "no-multiple-empty-lines": "off",
    "no-html-link-for-pages": "off"
  }
}
